in(?m,?i,?v) :- out(?m,?i,?v),
				!varDef(?m,?i,?v).                   
in(?m,?i,?v) :- varUse(?m,?i,?v).
out(?m,?i,?v) :- in(?m,?j,?v),next(?m,?i,?j).


dead(?m,?i) :- varDef(?m,?i,?v),
			   !out(?m,?i,?v).
			   

const_propagation(?m,?i,?v) :- constMove(?m,?i,?v,?c).			   			   
const_propagation(?m,?i,?v) :- !varDef(?m,?i,?v),
							   next(?m,?j,?i),
							   const_propagation(?m,?j,?v),
							   in(?m,?i,?v).
 
							